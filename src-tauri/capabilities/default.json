{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions and shell access for bd CLI",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "core:window:allow-close",
    "shell:allow-open",
    "shell:allow-execute",
    "core:menu:default",
    "dialog:allow-open"
  ]
}
